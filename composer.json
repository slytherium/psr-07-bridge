{
    "name": "zapheus/psr-07-bridge",
    "description": "Converts PSR-07 message to Zapheus HTTP and vice versa.",
    "keywords": ["http-message", "psr-7", "zapheus"],
    "homepage": "https://github.com/zapheus/psr-07-bridge",
    "license": "MIT",
    "authors": [
        {
            "name": "Rougin Royce Gutib",
            "email": "rougingutib@gmail.com",
            "homepage": "https://rougin.github.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "psr/http-message": "~1.0",
        "zapheus/zapheus": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.2",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Zapheus\\Bridge\\Psr\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zapheus\\Bridge\\Psr\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
