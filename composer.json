{
    "name": "zapheus/psr-07-bridge",
    "description": "PSR-07 bridge for Zapheus framework.",
    "keywords": ["bridge", "http-message", "psr-7", "zapheus"],
    "homepage": "https://zapheus.github.io/psr-bridge/",
    "license": "MIT",
    "authors":
    [
        {
            "email": "rougingutib@gmail.com",
            "homepage": "https://rougin.github.io/",
            "name": "Rougin Gutib",
            "role": "Software Engineer"
        }
    ],
    "require":
    {
        "php": ">=5.3.0",
        "psr/http-message": "~1.0",
        "zapheus/zapheus": "dev-master"
    },
    "require-dev":
    {
        "phpunit/phpunit": "~4.2|~5.7",
        "scrutinizer/ocular": "~1.1.0"
    },
    "autoload":
    {
        "psr-4":
        {
            "Zapheus\\Bridge\\Psr\\": "src"
        }
    },
    "autoload-dev":
    {
        "psr-4":
        {
            "Zapheus\\Bridge\\Psr\\": "tests"
        }
    },
    "scripts":
    {
        "test": "phpunit"
    },
    "extra":
    {
        "branch-alias":
        {
            "dev-master": "1.0-dev"
        }
    }
}